{
	"log": {
		"disabled": false,
		"level": "warn",
		"timestamp": true
	},
	"dns": {
		"servers": [
			{
				"tag": "global",
				"address": "https://1.1.1.1/dns-query",
				"address_resolver": "local",
				"strategy": "prefer_ipv6",
				"detour": "ssh-out"
			},
			{
				"tag": "local",
				"address": "8.8.8.8",
				"strategy": "prefer_ipv6",
				"detour": "direct-out"
			},
			{
				"tag": "block",
				"address": "rcode://success"
			}
		],
		"rules": [
			{
				"domain_suffix": [
					".ir",
					".edu",
					".cn",
					"microsoft.com",
					"bale.ai",
					"eitaa.com",
					"google.com"
				],
				"server": "local"
			}
		],
		"final": "global"
	},
	"inbounds": [
		{
			"type": "tun",
			"tag": "tun-in",
			"inet4_address": "172.43.0.1/30",
			"inet6_address": "fdfe:dcba:9876::1/126", 
			"mtu": 9000,
			"endpoint_independent_nat": true,
			"auto_route": true,
			"strict_route": true,
			"stack": "system",
			"sniff": true,
			"domain_strategy": "prefer_ipv6",
			"sniff_override_destination": true
		}
	],
	"outbounds": [
		{
			"type": "ssh",
			"tag": "ssh-out",
			"server": "$server_addr",
			"server_port": $server_port,
			"user": "$client_uuid",
			"private_key": $private_key,
			"host_key": [
				$host_key
			],
			"client_version": "SSH-2.0-OpenSSH_7.4p1"
		},
		{
			"type": "direct",
			"tag": "direct-out"
		},
		{
			"type": "dns",
			"tag": "dns-out"
		}
	],
	"route": {
		"rules": [
			{
				"domain_suffix": [
					".ir",
					".edu",
					".cn",
					"microsoft.com",
					"bale.ai",
					"eitaa.com",
					"google.com"
				],
				"outbound": "direct-out"
			},
			{
				"protocol": "dns",
				"outbound": "dns-out"
			},
			{
				"inbound": ["tun-in"],
				"outbound": "ssh-out"
			}
		],
		"auto_detect_interface": true
	}
}
